{
  "name": "sep",
  "private": false,
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.1",
  "main": "./dist/sep.umd.js",
  "module": "./dist/sep.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sep.es.js",
      "require": "./dist/sep.umd.js"
    },
    "./styles.css": "./dist/sep.css"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "npx @biomejs/biome lint --write ./src",
    "format": "npx @biomejs/biome check --write ./src",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.16",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@biomejs/biome": "2.3.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/addon-themes": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "10.4.23",
    "babel-loader": "10.0.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "json": "11.0.0",
    "lint-staged": "16.2.7",
    "msw": "2.12.7",
    "msw-storybook-addon": "2.0.6",
    "postcss": "8.5.6",
    "prop-types": "15.8.1",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --write --no-errors-on-unmatched",
      "biome lint --write --no-errors-on-unmatched --files-ignore-unknown=true",
      "biome format --write --no-errors-on-unmatched"
    ]
  }
}